<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <display-name></display-name>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
   <servlet>
   		<servlet-name>getAreaDis</servlet-name>
        <servlet-class>MyServlet.getAreaDis</servlet-class>
   </servlet>
   <servlet-mapping>
        <servlet-name>getAreaDis</servlet-name>
        <url-pattern>/getAreaDis</url-pattern>
   </servlet-mapping>
   
   <servlet>
   		<servlet-name>getGoods</servlet-name>
        <servlet-class>MyServlet.getGoods</servlet-class>
   </servlet>
   <servlet-mapping>
        <servlet-name>getGoods</servlet-name>
        <url-pattern>/getGoods</url-pattern>
   </servlet-mapping>
   
    <servlet>
   		<servlet-name>getGoodDetail</servlet-name>
        <servlet-class>MyServlet.getGoodDetail</servlet-class>
   </servlet>
   <servlet-mapping>
        <servlet-name>getGoodDetail</servlet-name>
        <url-pattern>/getGoodDetail</url-pattern>
   </servlet-mapping>
</web-app>
